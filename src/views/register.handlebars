        <input id="formRegisterInput" type="number" name="age" id="_age" />

        <label id="formRegisterLabel"  class="formRegisterLabel" for="_email">Email:</label>
        <input id="formRegisterInput" type="email" name="email" id="_email" />

        <label id="formRegisterLabel"  class="formRegisterLabel" for="_pass">Password:</label>
        <input id="formRegisterInput" type="password" name="password" id="_pass" />

        <button id="btnSubmit" type="submit">Registrarse</button>
